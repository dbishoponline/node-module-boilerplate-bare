{
  "name": "node-module-boilerplate-bare",
  "version": "1.0.0",
  "description": "A barebones boilerplate for building a new Node.js module",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "./scripts/postinstall.sh",
    "debug": "./scripts/debug.sh",
    "build": "./scripts/build.sh",
    "test": "./scripts/test.sh",
    "test:coverage": "nyc report",
    "start": "./scripts/start.sh",
    "stop": "./scripts/stop.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbishoponline/node-module-boilerplate-bare.git"
  },
  "author": "Dylan Bishop",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "debug": "^3.0.1",
    "nyc": "^11.2.1",
    "sinon": "^3.2.1",
    "tap-nyan": "^1.1.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "tap": true,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
